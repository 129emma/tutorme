    <title>Calender2</title>
    <style>

        * {
            box-sizing: border-box;
        }

        body {
            font-family: Verdana, sans-serif;
        }

        table {
            border-collapse: collapse;
            width: 100%;
            text-align: center;
        }

        tr:nth-child(even) {
            background-color: #f2f2f2
        }

        th {
            background-color: #4CAF50;
            color: white;
        }

        td {
            height: 40px;
        }

        td:hover {
            background-color: yellow;
        }

        .info {
            padding: 35px 25px;
            width: 100%;
            background: #1abc9c;
            text-align: center;
            margin: 0;
            color: white;
            font-size: 20px;
            text-transform: uppercase;
            letter-spacing: 3px;
        }


        .info .prev {
            float: left;
            padding-top: 10px;
        }

        .info .next {
            float: right;
            padding-top: 10px;
        }

        .selected {
            background-color: gold;
        }

        .available {
            background-color: greenyellow;
        }

        .requested {
            background-color: orangered;
        }

    </style>


</head>
